#! /bin/sh
set -e
#set -x

. /usr/share/debconf/confmodule

. /usr/lib/oar/setup/node.sh

PACKAGE=oar-node

case "$1" in
    configure)
        node_setup
        ;;

    abort-upgrade|abort-remove|abort-deconfigure)
        ;;
    
    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

errorinit() {
	return $?
}

#DEBHELPER#

exit 0


