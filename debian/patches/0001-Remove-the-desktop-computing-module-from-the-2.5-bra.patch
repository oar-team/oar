-
-
-do_start() {
-    begin_msg "Starting $DESC"
-    if pidofproc -p $PIDFILE > /dev/null; then
-        fail_msg " already running"
-        exit 1
-    fi
-    start_daemon -p "$PIDFILE" -n "-20" "$DAEMON" start
-    RET=$?
-    if [ "$RET" != 0 ]; then
-        fail_msg "Unable to start"
-    else
-        succ_msg
-    fi
-}
-
-do_stop() {
-    begin_msg "Stopping $DESC"
-    killproc -p $PIDFILE
-    RETVAL="$?"
-    if [ "$RETVAL" = 2 ]; then
-        fail_msg "Unable to stop $DESC"
-        exit 2
-    fi
