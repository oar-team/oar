-elif [ -f /etc/SuSE-release ]; then
-    system=suse
-elif [ -f /etc/gentoo-release ]; then
-    system=gentoo
-fi
-
-# Exit if the package is not installed
-[ -x "$DAEMON" ] || exit 0
-
-if [ -z "$NOLSB" ]; then
-    . /lib/lsb/init-functions
-    fail_msg() {
-        echo ""
-        log_failure_msg "$@"
-    }
-    warn_msg() {
-        log_warning_msg "$@"
-    }
-    succ_msg() {
-        log_success_msg "$@"
-    }
-    begin_msg() {
-        echo -n "$@:"
-    }
-else
